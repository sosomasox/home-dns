zone "home.arpa" IN {
    type forward;
    forward only;
    forwarders { 192.168.113.1; 192.168.113.2; };
};

zone "168.192.in-addr.arpa" IN {
    type forward;
    forward only;
    forwarders { 192.168.113.1; 192.168.113.2; };
};

zone "k8s" IN {
    type forward;
    forward only;
    forwarders { 192.168.115.245; };
};
